<h1>Edit <%=@landmark.name%></h1>

<form class="" action="/landmarks/<%=@landmark.id%>" method="post">
  <input type="hidden" name="_method" value="patch">

  <label for="name">Landmark Name: </label>
  <input type="text" id= "name" name="landmark[name]" value="<%=@landmark.name%>"><br></br>

  <label for = "year_completed">Year Completed: </label>
  <input type="text" id = "year_completed" name="landmark[year_completed]" value="<%=@landmark.year_completed%>"><br></br>

  <input type="submit" name="" value="Edit Landmark">
</form>
